def random():
    sqliteFile = 'Car_Database.sqlite'

    #connecting to car database
    con =  sql.connect (sqliteFile)
    cur = con.cursor()
    results = []
    cur.execute('''SELECT * FROM Cars''')
    for row in cur:
        results.append(row)
    con.close()
    j = []
    x = 0 
    for i in range (1):
        n = random.randint(1,24)
        x = n
        for y in results:
            if y != results[n]:
                pass
            else:
                j.append(results[n])
    return j




